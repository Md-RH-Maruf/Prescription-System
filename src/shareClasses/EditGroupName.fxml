<AnchorPane prefHeight="144.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shareClasses.EditGroupNameController">
   <children>
      <VBox layoutX="169.0" layoutY="83.0" prefHeight="400.0" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="lbl" alignment="BOTTOM_LEFT" prefHeight="28.0" prefWidth="317.0" text="Current Group Name : ">
